{
    "database": {
        "url": "postgres://$DB_USER:$DB_PASS@$DB_HOST:$DB_PORT/$DB_NAME?sslmode=$SSL_MODE"
    },
    "hash": {
        "bcrypt": "$BCRYPT_HASH"
    },
    "server": {
        "http": {
            "port": "$SERVER_PORT",
            "host": "$SERVER_HOST",
            "scheme": "$SERVER_SCHEME"
        }
    }
}